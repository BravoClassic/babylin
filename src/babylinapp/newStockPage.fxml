<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="babylinapp.NewStockPageController">
    <HBox prefHeight="500.0" prefWidth="300.0">
        <AnchorPane prefHeight="400.0" prefWidth="302.0">
            <Label alignment="CENTER" layoutX="99.0" layoutY="14.0" prefHeight="52.0" prefWidth="113.0" text="New Stock" textAlignment="CENTER">
                <font>
                    <Font size="17.0" />
                </font>
            </Label>
            <TextField fx:id="newStockNameProductPage" layoutX="53.0" layoutY="92.0" prefHeight="25.0" prefWidth="205.0" promptText="Stock Name" />
            <TextField fx:id="newStockPriceStockPage" layoutX="53.0" layoutY="137.0" prefHeight="25.0" prefWidth="205.0" promptText="Stock Unit Price(GHS)" />
            <TextField fx:id="newStockQuantityStockPage" layoutX="53.0" layoutY="187.0" prefHeight="25.0" prefWidth="205.0" promptText="Stock Quanity" />
            <TextArea fx:id="newStockDescStockPage" layoutX="53.0" layoutY="230.0" prefHeight="159.0" prefWidth="205.0" promptText="Stock Description" />
            <Button fx:id="newBtnStock" layoutX="94.0" layoutY="429.0" mnemonicParsing="false" onAction="#newStock" prefHeight="25.0" prefWidth="52.0" text="New" />
            <Button fx:id="cancel" layoutX="155.0" layoutY="429.0" mnemonicParsing="false" onAction="#goBack" prefHeight="25.0" prefWidth="52.0" text="Cancel" />
        </AnchorPane>
    </HBox>
</AnchorPane>
