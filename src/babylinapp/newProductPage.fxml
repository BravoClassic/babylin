<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="babylinapp.newProductPageClass">
   <HBox prefHeight="500.0" prefWidth="300.0">
      <AnchorPane prefHeight="400.0" prefWidth="302.0">
         <Label alignment="CENTER" layoutX="99.0" layoutY="14.0" onMousePressed="#newProduct" prefHeight="52.0"
                prefWidth="113.0" text="New Product" textAlignment="CENTER">
            <font>
               <Font size="17.0"/>
            </font>
         </Label>
         <TextField fx:id="newProductNameProductPage" layoutX="53.0" layoutY="92.0" prefHeight="25.0" prefWidth="205.0"
                    promptText="Product Name"/>
         <TextField fx:id="newProductPriceProductPage" layoutX="53.0" layoutY="137.0" prefHeight="25.0"
                    prefWidth="205.0" promptText="Product Unit Price(GHS)"/>
         <TextField fx:id="newProductQuantityProductPage" layoutX="53.0" layoutY="187.0" prefHeight="25.0"
                    prefWidth="205.0" promptText="Product Quanity"/>
         <TextArea fx:id="newProductDescProductPage" layoutX="53.0" layoutY="230.0" prefHeight="159.0" prefWidth="205.0"
                   promptText="Product Description"/>
         <Button layoutX="94.0" layoutY="429.0" mnemonicParsing="false" onAction="#newProduct" prefHeight="25.0"
                 prefWidth="52.0" text="New"/>
         <Button fx:id="cancel" layoutX="155.0" layoutY="429.0" mnemonicParsing="false" onAction="#goBack"
                 prefHeight="25.0" prefWidth="52.0" text="Cancel"/>
      </AnchorPane>
   </HBox>
</AnchorPane>
