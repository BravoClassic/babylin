<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="babylinapp.DeleteRawMaterialsPageController">
   <HBox prefHeight="300.0" prefWidth="300.0">
      <AnchorPane prefHeight="400.0" prefWidth="300.0">
