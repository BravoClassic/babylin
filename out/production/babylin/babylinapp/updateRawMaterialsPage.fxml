<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="babylinapp.UpdateRawMaterialsController">
   <HBox prefHeight="600.0" prefWidth="300.0">
      <AnchorPane prefHeight="400.0" prefWidth="308.0">
         <Button fx:id="update" layoutX="57.0" layoutY="539.0" mnemonicParsing="false" onAction="#update" prefHeight="25.0" prefWidth="63.0" text="Update" />
         <Button fx:id="cancel" layoutX="180.0" layoutY="539.0" mnemonicParsing="false" onAction="#goBack" prefHeight="25.0" prefWidth="63.0" text="Cancel" />
         <TextField fx:id="rawMaterialsName" layoutX="57.0" layoutY="171.0" prefHeight="25.0" prefWidth="186.0" promptText="Raw Material Name" />
         <TextField fx:id="rawMaterialsQuantity" layoutX="57.0" layoutY="225.0" prefHeight="25.0" prefWidth="186.0" promptText="Raw Material Quantity" />
         <TextField fx:id="rawMaterialsUnitPrice" layoutX="57.0" layoutY="275.0" prefHeight="25.0" prefWidth="186.0" promptText="Raw Material Unit Price" />
         <ComboBox fx:id="rawMaterialsList" layoutX="57.0" layoutY="80.0" onAction="#viewRawMaterial" prefHeight="25.0" prefWidth="186.0" promptText="Select Stock" />
         <TextArea fx:id="rawMaterialsDescription" layoutX="57.0" layoutY="317.0" prefHeight="200.0" prefWidth="186.0" promptText="Raw Material Description" />
         <Label alignment="CENTER" layoutX="94.0" layoutY="14.0" prefHeight="48.0" prefWidth="112.0" text="Update Stock">
            <font>
               <Font size="17.0" />
            </font>
         </Label>
         <TextField fx:id="rawMaterialId" layoutX="57.0" layoutY="124.0" prefHeight="25.0" prefWidth="186.0" promptText="Raw Material Id" />
      </AnchorPane>
   </HBox>
</AnchorPane>
